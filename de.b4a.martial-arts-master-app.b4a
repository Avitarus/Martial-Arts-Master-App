'  
'   adtap1.LoadAds  

	
	
	
	'Initialize the panels we use for the pages and put them in the container
	container.Initialize
	For i = 0 To 3
		Dim pan As Panel
		
		Select i
			Case 0
				pan = CreatePanel(TYPE_STARTSEITE, "MAMA")
				container.AddPage(pan,"Martial Art Master App" )
			Case 1
				pan = CreatePanel(TYPE_EBENE1, "Ebene 1")
				container.AddPage(pan,"Ebene 1")
			Case 2
				pan = CreatePanel(TYPE_EBENE2, "Ebene 2")
				container.AddPage(pan,"Ebene 2")
			'Case 3
